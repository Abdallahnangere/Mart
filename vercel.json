{
  "version": 2,
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/api/$1" },
    { "source": "/js/(.*)", "destination": "/public/js/$1" },
    { "source": "/(.*)\\.(png|jpg|jpeg|svg|ico|css)", "destination": "/public/$1.$2" },
    { "source": "/(.*)\\.html", "destination": "/public/$1.html" },
    { "source": "/(.*)", "destination": "/public/index.html" }
  ],
  "functions": {
    "api/*.js": {
      "maxDuration": 19
    }
  }
}
